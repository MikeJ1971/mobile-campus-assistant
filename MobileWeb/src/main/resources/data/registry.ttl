@prefix rdfs:       <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf:        <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:        <http://www.w3.org/2001/XMLSchema#> .
@prefix mca:        <http://org.ilrt.mca/registry#> .
@prefix dc:         <http://purl.org/dc/elements/1.1/> .
@prefix dcterms:    <http://purl.org/dc/terms/> .
@prefix geo:        <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix foaf:       <http://xmlns.com/foaf/0.1/> .


<mca://registry/>  rdf:type mca:Registry ;
    rdfs:label "Home" ;
    dc:description "Registry for the Mobile Campus Assistant, University of Bristol" ;
    mca:hasItem <mca://registry/today/> ;
    mca:hasItem <mca://registry/maps/> ;
    mca:hasItem <mca://registry/news/> ;
    mca:hasItem <mca://registry/events/> ;
    mca:hasItem <mca://registry/contacts/> ;
    mca:template <template://homepage.ftl> ;
    .

<mca://registry/today/> rdf:type mca:Group ;
    rdfs:label "Today" ;
    dc:description "Information for today" ;
    mca:hasItem <mca://registry/today/weather/> ;
    mca:hasItem <mca://registry/today/news/> ;
    mca:hasItem <mca://registry/today/pcavailability/> ;
    mca:hasItem <mca://registry/today/it/> ;
    mca:hasItem <mca://registry/today/events/> ;
    mca:order "1"^^xsd:integer ;
    mca:template <template://navigation.ftl> ;
    mca:style "today"^^xsd:string ;
    .

<mca://registry/today/weather/> rdf:type mca:HtmlFragment ;
    rdfs:label "Weather" ;
    dc:description "Today's weather in Bristol" ;
    rdfs:seeAlso <http://portal.bris.ac.uk/portal-weather/newXml> ;
    mca:order "1"^^xsd:integer ;
    mca:template <template://weather.ftl> ;
    mca:style "weather"^^xsd:string ;
    .

<mca://registry/today/news/> rdf:type mca:News ;
    rdfs:label "Today's News" ;
    dc:description "Today's News in the University of Bristol" ;
    mca:order "2"^^xsd:integer ;
    mca:template <template://newsList.ftl> ;
    mca:style "news"^^xsd:string ;
    .

<mca://registry/today/pcavailability/> rdf:type mca:HtmlFragment ;
    rdfs:label "PC Availability" ;
    dc:description "PC Availability" ;
    rdfs:seeAlso <http://is-freepcs.cse.bris.ac.uk/> ;
    mca:order "3"^^xsd:integer ;
    mca:template <template://pcavailability.ftl> ;
    mca:style "pcavailability"^^xsd:string ;
    .

<mca://registry/today/it/> rdf:type mca:News ;
    rdfs:label "IT Service Status" ;
    dc:description "IT Service Status provided by Information Services" ;
    rdfs:seeAlso <http://www.bris.ac.uk/is/news/status.rss> ;
    mca:order "4"^^xsd:integer ;
    mca:template <template://newsList.ftl> ;
    mca:style "news"^^xsd:string ;
    .

<mca://registry/today/events/> rdf:type mca:EventCalendar ;
    rdfs:label "Today's events" ;
    dc:description "Events happening today" ;
    mca:order "5"^^xsd:integer ;
    mca:eventlist "TODAY" ;
    mca:template <template://eventList.ftl> ;
    mca:style "events"^^xsd:string ;
    .

<mca://registry/maps/> rdf:type mca:Group ;
    rdfs:label "Maps" ;
    dc:description "Useful maps of the campus" ;
    mca:hasItem <mca://registry/maps/wireless/> ;
    mca:hasItem <mca://registry/maps/precinct/> ;
    mca:hasItem <mca://registry/maps/busstops/> ;
    mca:order "2"^^xsd:integer ;
    mca:template <template://navigation.ftl> ;
    mca:style "maps"^^xsd:string ;
    .

<mca://registry/maps/busstops/> rdf:type mca:ActiveMapSource ;
    rdfs:label "Live Bus Departures" ;
    dc:description "Bus stops and live departure information." ;
    mca:urlStem "busproxy/?id=" ;
    mca:markers "js/busstops.json" ;
    mca:icon "images/bus.png" ;
    mca:template <template://activeMap.ftl> ;
    mca:order "3"^^xsd:integer ;
    geo:lat "51.4585695818037"^^xsd:double ;
    geo:long "-2.60187349563486"^^xsd:double ;
    mca:style "maps"^^xsd:string ;
    .

<mca://registry/maps/wireless/> rdf:type mca:KmlMapSource ;
    rdfs:label "Wireless Hotspots" ;
    dc:description "The location of wireless hotspots across the campus." ;
    rdfs:seeAlso <http://wireless.bris.ac.uk/maps/wireless-core.kml> ;
    mca:template <template://kmlMap.ftl> ;
    mca:order "2"^^xsd:integer ;
    geo:lat "51.4585695818037"^^xsd:double ;
    geo:long "-2.60187349563486"^^xsd:double ;
    mca:style "maps"^^xsd:string ;
    .

<mca://registry/maps/precinct/> rdf:type mca:KmlMapSource ;
    rdfs:label "University Precinct and Residences" ;
    dc:description "University precinct and residences." ;
    rdfs:seeAlso <http://maps.google.com/maps/ms?ie=UTF8&hl=en&msa=0&output=kml&msid=111005880056449945013.0004718d9aaef89003fc6> ;
    mca:template <template://kmlMap.ftl> ;
    mca:order "1"^^xsd:integer ;
    geo:lat "51.4585695818037"^^xsd:double ;
    geo:long "-2.60187349563486"^^xsd:double ;
    mca:style "maps"^^xsd:string ;
    .

<mca://registry/contacts/> rdf:type mca:Group ;
    rdfs:label "Contacts" ;
