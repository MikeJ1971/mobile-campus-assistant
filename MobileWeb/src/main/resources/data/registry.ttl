    mca:order "5"^^xsd:integer ;
    mca:hasItem <mca://registry/contacts/emergency/> ;
    mca:hasItem <mca://registry/contacts/university/> ;
    mca:hasItem <mca://registry/contacts/welfare/> ;
    mca:hasItem <mca://registry/contacts/directory/> ;
