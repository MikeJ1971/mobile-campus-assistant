PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX sy: <http://purl.org/rss/1.0/modules/syndication/>
PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX taxo: <http://purl.org/rss/1.0/modules/taxonomy/>
PREFIX content: <http://purl.org/rss/1.0/modules/content/>
PREFIX rss: <http://purl.org/rss/1.0/>
PREFIX mca: <http://org.ilrt.mca/registry#>
CONSTRUCT {
    ?id rdf:type <http://org.ilrt.mca/registry#NewsItem> .
    ?id mca:template <template://newsItem.ftl> ; .
    ?id rdfs:seeAlso ?seeAlso .
    ?seeAlso mca:hasItem ?s .
    ?s ?p ?o .
} WHERE {
    GRAPH ?graph {
        ?s ?p ?o .
    }
}